{
  "name": "stackoverflow",
  "matches": [
    {
      "match": "(https?://)?([\\w.]*\\.)?stackoverflow.com/users/({PROFILE_ID})(/[A-Za-z0-9_\\-\\.]+)?/?",
      "group": 3,
      "pattern": "https://stackoverflow.com/users/{PROFILE_ID}"
    },
    {
      "match": "({PROFILE_ID})",
      "group": 1
    }
  ]
} 