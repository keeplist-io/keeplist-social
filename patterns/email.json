{
  "name": "email",
  "pattern": "[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,}",
  "hint": "username@domain.com",
  "matches": [
    {
      "match": "mailto:({PROFILE_ID})/?",
      "group": 1,
      "pattern": "mailto:{PROFILE_ID}"
    },
    {
      "match": "({PROFILE_ID})",
      "group": 1
    }
  ],
  "icon": {
    "iconType": "fa",
    "value": "envelope",
    "FaStyle": "solid"
  }
} 